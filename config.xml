<widget id="A6075845920844" version="0.0.1">    <name>haitianApp</name>    <description>        Example For APICloud.    </description>    <author email="349017128@qq.com" href="http://www.apicloud.com">        LinFeng    </author>    <content src="index.html"/>    <access origin="*"/>    <!-- preference：本应用的一些全局设置或者属性，启动及运行过程中会随时参考这些属性 -->    <!-- 页面是否弹动 -->    <preference name="pageBounce" value="false"/>    <!-- 背景颜色 -->    <preference name="appBackground" value="rgba(255,255,255,1)"/>    <preference name="windowBackground" value="rgba(255,255,255,1)"/>    <preference name="frameBackgroundColor" value="rgba(255,255,255,1)"/>    <!-- 是否显示横向滚动条 -->    <preference name="hScrollBarEnabled" value="false"/>    <!-- 是否显示纵向滚动条 -->    <preference name="vScrollBarEnabled" value="false"/>    <!-- 启动页是否自动隐藏 -->    <preference name="autoLaunch" value="true"/>    <!-- 配置是否全屏运行 -->    <preference name="fullScreen" value="false"/>    <!-- 自动检测更新，true的情况下，自动与云端握手 -->    <preference name="autoUpdate" value="true"/>    <!-- 增量更新 -->    <preference name="smartUpdate" value="false"/>    <!-- 调试BUG模式，开启的情况下，BUG会直接出现在手机上。 -->    <preference name="debug" value="true"/>    <!-- 配置是否开启沉浸式效果 -->    <preference name="statusBarAppearance" value="true"/>    <!-- 权限配置，如拨打电话、短信、定位等 -->    <permission name="readPhoneState"/>    <permission name="camera"/>    <permission name="record"/>    <permission name="location"/>    <permission name="fileSystem"/>    <permission name="internet"/>    <permission name="bootCompleted"/>    <permission name="hardware"/></widget>